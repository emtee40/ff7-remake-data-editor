{
  "name": "ff7r-data-editor",
  "version": "1.0.0-a.1",
  "description": "Final Fantasy VII Remake Data Editor",
  "private": true,
  "main": "main/index.js",
  "engines": {
    "node": "^12 || ^14 || >=16",
    "npm": ">=7"
  },
  "scripts": {
    "build": "electron-packager . --out dist --overwrite",
    "eslint": "eslint --fix .",
    "lint": "run-s eslint prettier",
    "prepare": "husky install",
    "prettier": "prettier --loglevel warn -w .",
    "preversion": "npm run lint",
    "start": "npx electron main/index.js",
    "version": "git add CHANGELOG.md"
  },
  "author": "Jordan Tucker <jordanbtucker@gmail.com> (https://github.com/jordanbtucker)",
  "license": "MIT",
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "electron": "^16.0.5",
    "electron-packager": "^15.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  }
}
